<p id="notice"><%= notice %></p>
<div class="container">
  <h1>Nodes</h1>

  <table>
    <thead>
      <tr>
        <th>Data In Encrypted format</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @nodes.each do |node| %>
        <tr>
          <td><%= "dDMwQS9MMk" + eval(node.data).values.join.gsub(" ", '') + "Z2VJY1o3aGtwaC9Wb0wvSn" %></td>
          <td><%= link_to 'Show', node %></td>
          <td><%= link_to 'Edit', edit_node_path(node) %></td>
          <td><%= link_to 'Destroy', node, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Node', new_node_path %>
</div>
